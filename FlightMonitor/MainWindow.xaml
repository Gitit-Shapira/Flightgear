        xmlns:local="clr-namespace:FlightMonitor"
        xmlns:con="clr-namespace:FlightMonitor" xmlns:win32="clr-namespace:Microsoft.Win32;assembly=PresentationFramework"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" AllowDrop="True" >
